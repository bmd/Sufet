{
  "name": "bmd/sufet",
  "description": "A content negotiation framework for PHP 7",
  "license": "MIT",
  "authors": [
    {
      "name": "Brendan Maione-Downing",
      "email": "b.maionedowning@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "psr/http-message": "1.*",
    "symfony/http-foundation": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "zendframework/zend-diactoros": "*",
    "satooshi/php-coveralls": "~1.0",
    "phpunit/phpcov": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Sufet\\": "src/"
    }
  },
  "config": {
    "secure-http": false,
    "vendor-dir": "vendor"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
